# Path to the input log file
INPUT_FILE_PATH=../input/sample.log

# Number of lines per chunk for processing
CHUNK_SIZE=1000

# Maximum number of chunks queued at a time
QUEUE_MAX_SIZE=10

# File to persist last read position (parser state)
STATE_FILE=../state/parser_state.json

# File to persist already seen line hashes
SEEN_FILE=../state/seen_lines.pkl

# Time in seconds to wait when polling for new lines
POLL_INTERVAL=0.5

# Number of worker processes for parsing
NUM_PROCESSES=3